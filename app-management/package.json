{
  "name": "app-management",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:bs": "nest build batch-server",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "orm:migration:create": "npm run build && ts-node ./node_modules/typeorm/cli migration:create -o  ./migrations/migratoin",
    "orm:migration:generate": "npm run build && ts-node ./node_modules/typeorm/cli migration:generate -o ./migrations/migratoin -d data-source.ts",
    "orm:migration:run": "ts-node ./node_modules/typeorm/cli migration:run  -d data-source.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:bs:dev": "nest start batch-server --watch",
    "start:debug": "PORT=9000 nest start --debug --watch",
    "start:prod": "node dist/apps/app-management/main",
    "start:bs:prod": "node dist/apps/batch-server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app-management/test/jest-e2e.json",
    "tsc": "tsc"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.398.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-sns": "^3.413.0",
    "@aws-sdk/lib-dynamodb": "^3.398.0",
    "@aws-sdk/s3-request-presigner": "^3.465.0",
    "@google-cloud/firestore": "^7.3.1",
    "@google-cloud/text-to-speech": "^5.1.0",
    "@google-cloud/translate": "^8.2.0",
    "@google/generative-ai": "^0.12.0",
    "@googleapis/youtube": "^11.0.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "axios": "^1.7.9",
    "bull": "^4.11.3",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.0",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "nest-cli": "^0.0.5",
    "openai": "^4.47.2",
    "pg": "^8.11.2",
    "probe-image-size": "^7.2.3",
    "puppeteer": "^21.3.6",
    "puppeteer-screen-recorder": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.22",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/probe-image-size": "^7.2.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^@apps/app-management/(.*)$": "<rootDir>/apps/app-management/src/$1",
      "^@app/shared/(.*)$": "<rootDir>/libs/shared/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  }
}
